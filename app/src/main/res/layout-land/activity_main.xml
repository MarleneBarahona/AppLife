<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#000000"
    android:columnCount="7"
    android:rowCount="5"
    tools:context="com.example.yanirayanes.taller2.MainActivity">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_column="0"
        android:layout_row="0"
        />
    <ImageView
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_row="1"
        />
    <LinearLayout
        android:background="#04B4AE"
        android:padding="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_row="1"
        android:orientation="vertical"
        >
        <ImageView
            android:id="@+id/imgconfiguracion"
            android:layout_width="150dp"
            android:layout_height="90dp"
            android:src="@drawable/conf"/>
        <TextView
            android:id="@+id/configuracion2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/configuracion"
            android:gravity="center"/>
    </LinearLayout>
    <ImageView
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:layout_row="1"
        />
    <LinearLayout
        android:background="#01DF3A"
        android:padding="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="3"
        android:layout_row="1"
        android:orientation="vertical"
        >
        <ImageView
            android:id="@+id/imgperfil"
            android:layout_width="150dp"
            android:layout_height="90dp"
            android:src="@drawable/perf"/>
        <TextView
            android:id="@+id/perfil2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/perfil"
            android:gravity="center"/>
    </LinearLayout>
    <ImageView
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_column="4"
        android:layout_row="1"
        />
    <LinearLayout
        android:background="#F5A9A9"
        android:padding="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="5"
        android:layout_row="1"
        android:orientation="vertical"
        >
        <ImageView
            android:id="@+id/imgprsupuesto"
            android:layout_width="150dp"
            android:layout_height="90dp"
            android:src="@drawable/pres"/>
        <TextView
            android:id="@+id/presupuesto2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/presupuesto"
            android:gravity="center"/>
    </LinearLayout>
    <ImageView
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_column="6"
        android:layout_row="1"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_column="0"
        android:layout_row="2"
        />
    <ImageView
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_row="3"
        />
    <LinearLayout
        android:background="#3104B4"
        android:padding="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_row="3"
        android:orientation="vertical"
        >
        <ImageView
            android:id="@+id/imgfav"
            android:layout_width="150dp"
            android:layout_height="90dp"
            android:src="@drawable/fav"/>
        <TextView
            android:id="@+id/favoritos2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/favoritos"
            android:gravity="center"/>
    </LinearLayout>
    <ImageView
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:layout_row="3"
        />
    <LinearLayout
        android:background="#A901DB"
        android:padding="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="3"
        android:layout_row="3"
        android:orientation="vertical"
        >
        <ImageView
            android:id="@+id/imgingresos"
            android:layout_width="150dp"
            android:layout_height="90dp"
            android:src="@drawable/ing"/>
        <TextView
            android:id="@+id/ingresos2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/ingresos"
            android:gravity="center"/>
    </LinearLayout>
    <ImageView
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_column="4"
        android:layout_row="3"
        />
    <LinearLayout
        android:background="#B4045F"
        android:padding="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="5"
        android:layout_row="3"
        android:orientation="vertical"
        >
        <ImageView
            android:id="@+id/imgegresos"
            android:layout_width="150dp"
            android:layout_height="90dp"
            android:src="@drawable/egr"/>
        <TextView
            android:id="@+id/egresos2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/egresos"
            android:gravity="center"/>
    </LinearLayout>
    <ImageView
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_column="6"
        android:layout_row="3"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_column="0"
        android:layout_row="4"
        />
</GridLayout>
